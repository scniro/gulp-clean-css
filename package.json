{
  "name": "gulp-clean-css",
  "description": "Minify css with clean-css.",
  "homepage": "https://github.com/scniro/gulp-clean-css#readme",
  "version": "4.0.0",
  "author": "scniro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scniro/gulp-clean-css/issues",
