  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-string": "1.5.0",
    "coveralls": "3.0.2",
    "express": "4.16.4",
    "fancy-log": "1.3.2",
    "gulp": "4.0.0",
    "gulp-concat": "2.6.1",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.4",
    "mocha": "5.2.0",
